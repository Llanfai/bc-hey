{
    "compilerOptions": {
        "target": "es2017",
        "module": "NodeNext",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "types": [
            "node"
        ],
        "moduleResolution": "nodenext",
        "baseUrl": ".",
        "paths": {
            "redis": [
                "node_modules/redis"
            ]
        }
    }
}